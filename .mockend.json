{
    "Category": {
        "slug": {
            "regexp": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
        },
        "title": {
            "string": {}
        },
        "image": {
            "regexp": "https://picsum\\.photos/seed/[0-9]{5}/1920/1080"
        },
        "products": {
            "hasMany": "Product"
        }
    },
    "Brand": {
        "title": {
            "string": {}
        },
        "image": {
            "regexp": "https://picsum\\.photos/seed/[0-9]{5}/1920/1080"
        },
        "products": {
            "hasMany": "Product"
        }
    },
    "Product": {
        "title": {
            "string": {}
        },
        "price": {
            "int": {
                "min": 20,
                "max": 1000
            }
        },
        "image": {
            "regexp": "https://picsum\\.photos/seed/[0-9]{5}/1920/1080"
